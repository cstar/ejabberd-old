# $Id$

CC = @CC@ 
CFLAGS = @CFLAGS@ @ERLANG_CFLAGS@
CPPFLAGS = @CPPFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@ @ERLANG_LIBS@

SUBDIRS = 

OUTDIR = ..
EFLAGS = -I .. -pz ..
OBJS   = \
	$(OUTDIR)/mod_muc.beam \
	$(OUTDIR)/mod_muc_log.beam \
	$(OUTDIR)/mod_muc_room.beam

all:    $(OBJS)

$(OUTDIR)/%.beam:       %.erl
	@ERLC@ -W $(EFLAGS) -o $(OUTDIR) $<

clean:
	rm -f $(OBJS)

distclean: clean
	rm -f Makefile

TAGS:
	etags *.erl

