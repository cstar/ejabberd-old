# $Id$

CC = @CC@
CFLAGS = @CFLAGS@
CPPFLAGS = @CPPFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

INCLUDES = @ERLANG_CFLAGS@

LIBDIRS = @ERLANG_LIBS@

SUBDIRS = 


OUTDIR = ..
EFLAGS = -I .. -pz ..
OBJS   = \
	$(OUTDIR)/mod_pubsub.beam

all:    $(OBJS)

$(OUTDIR)/%.beam:       %.erl
	erlc -W $(EFLAGS) -o $(OUTDIR) $<


clean:
	rm -f $(OBJS)

distclean: clean
	rm -f Makefile

TAGS:
	etags *.erl

